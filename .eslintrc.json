{
    "extends": "airbnb",
    "plugins": ["prettier"],
    "rules": {
        "no-undef": 0,
        "no-console": 0,
        "react/jsx-filename-extension": 0,
        "comma-dangle": 0,
        "arrow-parens": 0,
        "operator-linebreak": 0,
        "no-use-before-define": 0,
        "react/jsx-one-expression-per-line": 0,
        "react/prefer-stateless-function": 0,
        "no-param-reassign": 0,
        "prettier/prettier": "warn"

    }
}